<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="escstring" nsURI="http://www.eclipse.org/gef/dot/internal/language/DotEscString"
    nsPrefix="escstring">
  <eClassifiers xsi:type="ecore:EClass" name="EscString">
    <eStructuralFeatures xsi:type="ecore:EReference" name="lines" upperBound="-1"
        eType="#//JustifiedText" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JustifiedText">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="text" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="justification" eType="#//Justification"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Justification">
    <eLiterals name="CENTERED" literal="\n"/>
    <eLiterals name="LEFT" value="1" literal="\l"/>
    <eLiterals name="RIGHT" value="2" literal="\r"/>
  </eClassifiers>
</ecore:EPackage>
