<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.emf.facet.common.ui.menu.examples">
         <command
               commandId="org.eclipse.emf.facet.widgets.table.examples.library.core.openWriterTable"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.eclipse.emf.facet.widgets.table.examples.library.core.internal.handler.OpenWriterTableHandler"
            id="org.eclipse.emf.facet.widgets.table.examples.library.core.openWriterTable"
            name="%openAWriterTable">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.eclipse.emf.facet.widgets.table.examples.library.core.internal.StartUp">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.eclipse.emf.facet.widgets.table.examples.library.core.internal.view.WriterView"
            id="org.eclipse.emf.facet.widgets.table.examples.library.core.writer.view"
            name="%writers"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.emf.facet.widgets.table.examples.library.core.writer.view">
         <command
               commandId="org.eclipse.emf.facet.widgets.table.exportCommand"
               icon="icons/export.gif"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.loadCustomizationCommand"
               icon="icons/uiCustom.gif"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.loadFacetCommand"
               icon="icons/facet.gif"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.hideColumnCommand"
               icon="icons/select_columns_to_hide.png"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.createNewElementCommand"
               icon="IMG_OBJ_ADD"
               style="pulldown">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.sortColumnByTypeCommand"
               icon="icons/sort_links_by_type.gif"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.sortColumnByNameAToZCommand"
               icon="icons/sort_columns_by_name_A_to_Z.gif"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.sortColumnByNameZToACommand"
               icon="icons/sort_columns_by_name_Z_to_A.gif"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.setPresentabilityQueryCommand"
               icon="icons/presentabilityQuery.gif"
               style="push">
         </command>
         <command
               commandId="org.eclipse.emf.facet.widgets.table.addColumnsCommand"
               icon="icons/addColumn.png"
               style="push">
         </command>
      </menuContribution>
   </extension>
</plugin>
